				ins[i+1][3]=chardel(ins[i+1][3])					
				if ins[i][1]== ins[i+1][2] or ins[i+1][3]==ins[i][1] or ins[i][1]==ins[i+1][1] or ins[i+1][1]==ins[i][2] or ins[i+1][1]==ins[i][3]:
					time1+=1
					time.append(time1)
				else:
					time1+=1
					time.append(time1)
			elif ins[i+1][0]=='lw':
				ins[i+1][1]=chardel(ins[i+1][1])
