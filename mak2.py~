def process(l1,flagult):
        k=[]
        for i in range(len(l1)):
                if l1[i]==[] and flagult[i]==0:
                        k.append(i)

        return k
for fun in l1:
                s1=process(l1,flagult)
      #          print s1,"Each Iteration OOO"
                l3.append(s1)
         #       print flagult,"Flagg"
                for s in s1:
                        position=s
                        flagult[position]=1
                        l1[position].append(-1)
        #                print l1
                        for k in l1:
                                p1=l1.index(k)
                                for j in range(len(k)):
                                        try:
                                                if k[j]==position and flagult[p1]==0:
