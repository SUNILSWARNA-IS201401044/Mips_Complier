def func(l5,j,time):
	lu=[]
	maxi=0
#	par=0
#	ti=0
	global par
	global ti
	for k in range(len(l5)):
		if j==k:
			for m in l5[k]:
				lu.append(m)
			print lu,"List"
	for n in lu:
		if ins[n][0]=='lw' or ins[n][0]=='sw':
			if maxi<time[n]:
				par=n
				maxi=time[n]
				ti=2
		else:
			if maxi<time[n]:
				par=n
				maxi=time[n]
				ti=1
	time11=time[par]+ti
	return time11	 	

