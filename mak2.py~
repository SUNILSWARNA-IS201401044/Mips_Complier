def chardel(k):
	s=''
	if k.endswith(')'):
		s=s+k[len(k)-4]+k[len(k)-3]+k[len(k)-2]
	else:
		s=k
	return s

def adddep(add1,add2,add3,k):
	j=0
	for i in lis3[k:]:
		r=lis3.index(i)
		sum1=0
