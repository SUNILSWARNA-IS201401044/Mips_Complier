#####----------GUI----------######

from Tkinter import *
import random
import sys
root=[]
root=random.sample(xrange(1000),100)
root[0]=Tk()
root[0].title('Assembler')
root[0].geometry('300x300')
top=Frame(root[0])
top.pack()

lb=Label(top,text="Currently Running File:"+' '+sys.argv[1])
lb.pack()

#RAW Dep
def PrintRAW():
	root[1]=Tk()
	top1=Frame(root[1])
	top1.pack()
	text=Text(top1)
	text.insert(INSERT,"RAW dependencies\n")
	if dict1["RAW"]==[]:
		text.insert(END,"NONE")
		text.pack()
	else:
		for i in dict1["RAW"]:
			text.insert(INSERT,' '.join(i)+'\n')
		text.insert(END,"End of RAW dependencies")
		text.pack()
but1=Button(top,text="RAW Dependencies",command=PrintRAW)
but1.pack()

#WAR Dep
def PrintWAR():
	root[2]=Tk()
	top2=Frame(root[2])
	top2.pack()
	text=Text(top2)
	text.insert(INSERT,"WAR dependencies\n")
	if dict1["WAR"]==[]:
		text.insert(END,"NONE")
		text.pack()
	else:
		for i in dict1["WAR"]:
			text.insert(INSERT,' '.join(i)+'\n')
		text.insert(END,"End of WAR dependencies")
		text.pack()
but2=Button(top,text="WAR Dependencies",command=PrintWAR)
but2.pack()

#WAW Dep
def PrintWAW():
	root[3]=Tk()
	top3=Frame(root[3])
	top3.pack()
	text=Text(top3)
	text.insert(INSERT,"WAW dependencies\n")
	if dict1["WAW"]==[]:
		text.insert(END,"NONE")
		text.pack()
	else:
		for i in dict1["WAW"]:
			text.insert(INSERT,' '.join(i)+'\n')
		text.insert(END,"End of WAW dependencies")
		text.pack()
but3=Button(top,text="WAW Dependencies",command=PrintWAW)
but3.pack()

#Inorder seq
def PrintIn():
	root[4]=Tk()
	top4=Frame(root[4])
	top4.pack()
	text=Text(top4)
	text.insert(INSERT,"Instruction"+'          '+"Time Taken"+'\n')
	text.insert(INSERT,'--------------------------------------------------------------------'+'\n')	
	for i in range(len(lis3)):
		if len(lis3[i])<=3:
			text.insert(INSERT,lis3[i][0]+' '+lis3[i][1]+','+lis3[i][2]+'                   '+str(time[i])+'\n')
			text.insert(INSERT,'--------------------------------------------------------------------'+'\n')
		else:
			text.insert(INSERT,lis3[i][0]+' '+lis3[i][1]+','+lis3[i][2]+','+lis3[i][3]+'        '+str(time[i])+'\n')
			text.insert(INSERT,'--------------------------------------------------------------------'+'\n')
	text.insert(END,'\n')
	text.pack()
but4=Button(top,text="Inorder",command=PrintIn)
but4.pack()

def CPin():
	root[6]=Tk()
	top6=Frame(root[6])
	top6.pack()
	text=Text(top6)
	text.insert(INSERT,"CPI of this InOrder Sequence is:\n")
	text.insert(END,CPIinorder)
	text.pack()
but6=Button(top,text="CPI of Inorder",command=CPin)
but6.pack()

def CPOut():
	root[7]=Tk()
	top7=Frame(root[7])
	top7.pack()
	text=Text(top7)
	text.insert(INSERT,"CPI of this Out of Order Sequence is:\n")
	text.insert(END,CPIOorder)
	text.pack()
but7=Button(top,text="CPI of OOO",command=CPOut)
but7.pack()


s=list(l3)
